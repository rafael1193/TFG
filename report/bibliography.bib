@article{asada_mobile_2003,
	title = {Mobile monitoring with wearable photoplethysmographic biosensors},
	volume = {22},
	issn = {0739-5175},
	doi = {10.1109/MEMB.2003.1213624},
	abstract = {We address both technical and clinical issues of wearable biosensors ({WBS}). First, design concepts of a {WBS} are presented, with emphasis on the ring sensor developed by the author's group at {MIT}. The ring sensor is an ambulatory, telemetric, continuous health-monitoring device. This {WBS} combines miniaturized data acquisition features with advanced photoplethysmographic ({PPG}) techniques to acquire data related to the patient's cardiovascular state using a method that is far superior to existing fingertip {PPG} sensors. In particular, the ring sensor is capable of reliably monitoring a patient's heart rate, oxygen saturation, and heart rate variability. Technical issues, including motion artifact, interference with blood circulation, and battery power issues, are addressed, and effective engineering solutions to alleviate these problems are presented. Second, based on the ring sensor technology the clinical potentials of {WBS} monitoring are addressed.},
	pages = {28--40},
	number = {3},
	journaltitle = {{IEEE} Engineering in Medicine and Biology Magazine},
	author = {Asada, H.H. and Shaltis, P. and Reisner, A. and Rhee, Sokwoo and Hutchinson, R.C.},
	date = {2003-05},
	keywords = {ambulatory telemetric continuous health-monitoring device, battery power, Biomedical monitoring, biomedical telemetry, biomedical transducers, Biosensors, blood circulation, blood pressure measurement, cardiology, cardiovascular monitoring, cardiovascular state, cardiovascular system, Data acquisition, electroencephalography, electromyography, heart rate, Heart rate measurement, heart rate variability, miniaturized data acquisition features, mobile monitoring, motion artifact, oxygen saturation, patient monitoring, plethysmography, ring sensor, Sensor phenomena and characterization, technical issues, Telemetry, wearable photoplethysmographic biosensors, Wearable sensors},
	file = {IEEE Xplore Abstract Record:/home/rafael/.mozilla/firefox/d35ziwi1.default/zotero/storage/ARS93994/articleDetails.html:text/html;IEEE Xplore Abstract Record:/home/rafael/.mozilla/firefox/d35ziwi1.default/zotero/storage/K33NB5TH/articleDetails.html:text/html}
}

@inproceedings{jeong_implementation_2012,
	title = {Implementation of pulse wave monitoring system using wearable {PPG} measurement system in daily life},
	abstract = {The conventional Photoplethymosgraphy ({PPG}) measurements take place where patient is required to clip the photo sensor on the finger and therefore patient's movement is restricted. In this paper, a wireless ear hook-type {PPG} measurement system using Bluetooth technology is proposed. The wireless ear hook-type {PPG} measurement system consists of a photo sensor, a {PPG} signal conditioning circuit, and an integrated Bluetooth transceiver. Photo sensor is placed on the ear lobe while there is a min i hooker attached to hold the sensor in place. {PPG} signal sensed from the earlobe is amplified through cascaded amplifiers analog circuits is amplified and then filter using band-pass technique. {PPG} signal is 12 bits sampling at frequency of 100Hz. An android application is developed for real time and continuous {PPG} signal monitoring.},
	eventtitle = {2012 7th International Conference on Computing and Convergence Technology ({ICCCT})},
	pages = {309--312},
	booktitle = {2012 7th International Conference on Computing and Convergence Technology ({ICCCT})},
	author = {Jeong, Sun-A. and Lee, Young-Dong and Jeong, Do-Un and Hwang, Gi-Hyun},
