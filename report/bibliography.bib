@article{wang_multichannel_2007,
	title = {Multichannel Reflective {PPG} Earpiece Sensor With Passive Motion Cancellation},
	volume = {1},
	issn = {1932-4545},
	doi = {10.1109/TBCAS.2007.910900},
	abstract = {This paper addresses the design considerations of a novel earpiece photoplethymograph ({PPG}) sensor and its in-situ evaluation results. The device is encapsulated with multiple {LEDs} and photodiodes based on a reflective {PPG} design. A compact and low power circuitry was developed for signal control and conditioning. {PPG} signals with an averaged ac/dc ratio of 0.001-0.01 and 10\% relative strength (compared to finger-based approach) were recorded from the superior and posterior auricular skins. The integrity of {PPG} signal and accuracy of heart rate detection were evaluated and the results showed that with adequate optical shielding and the proposed passive motion cancellation, the device was able to reliably detect heart rate both during rest and moderate exercise. The proposed sensor design is low power, easy to wear compared to conventional earlobe {PPG} devices.},
	pages = {235--241},
	number = {4},
	journaltitle = {{IEEE} Transactions on Biomedical Circuits and Systems},
	author = {Wang, Lei and Lo, B.P.L. and Yang, Guang-Zhong},
	date = {2007-12},
	keywords = {auricular skins, bio-optics, body sensor networks, cardiology, Circuits, earpiece, heart rate, heart rate detection, heart-rate monitoring, {LED}, Light emitting diodes, low power circuitry, Motion detection, multichannel reflective {PPG} earpiece sensor, Optical devices, Optical recording, optical sensors, optical shielding, passive motion cancellation, photodiodes, photoplethymograph sensor, photoplethysmograph ({PPG}), plethysmography, signal conditioning, signal control, skin, Wearable sensors},
	file = {IEEE Xplore Abstract Record:/home/rafael/.mozilla/firefox/d35ziwi1.default/zotero/storage/C2FQSNM7/abs_all.html:text/html}
