@article{kumar_bench_2015,
	title = {From the Bench to the Field in Low-Cost Diagnostics: Two Case Studies},
	volume = {54},
	rights = {© 2015 {WILEY}-{VCH} Verlag {GmbH} \& Co. {KGaA}, Weinheim},
	issn = {1521-3773},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/anie.201411741/abstract},
	doi = {10.1002/anie.201411741},
	shorttitle = {From the Bench to the Field in Low-Cost Diagnostics},
	abstract = {Despite the growth of research in universities on point-of-care ({POC}) diagnostics for global health, most devices never leave the laboratory. The processes that move diagnostic technology from the laboratory to the field—the processes intended to evaluate operation and performance under realistic conditions—are more complicated than they might seem. Two case studies illustrate this process: the development of a paper-based device to measure liver function, and the development of a device to identify sickle cell disease based on aqueous multiphase systems ({AMPS}) and differences in the densities of normal and sickled cells. Details of developing these devices provide strategies for forming partnerships, prototyping devices, designing studies, and evaluating {POC} diagnostics. Technical and procedural lessons drawn from these experiences may be useful to those designing diagnostic tests for developing countries, and more generally, technologies for use in resource-limited environments.},
	pages = {5836--5853},
	number = {20},
	journaltitle = {Angewandte Chemie International Edition},
	shortjournal = {Angew. Chem. Int. Ed.},
	author = {Kumar, Ashok A. and Hennek, Jonathan W. and Smith, Barbara S. and Kumar, Shailendra and Beattie, Patrick and Jain, Sidhartha and Rolland, Jason P. and Stossel, Thomas P. and Chunda-Liyoka, Catherine and Whitesides, George M.},
	urldate = {2015-07-02},
	date = {2015-05-11},
	langid = {english},
	keywords = {field trials, liver injury, point-of-care diagnostics, sickle cell disease, technologytranslation},
	file = {Snapshot:/home/rafael/.mozilla/firefox/d35ziwi1.default/zotero/storage/UVXZP56D/abstract.html:text/html}
}

